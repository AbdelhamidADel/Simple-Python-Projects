def show_universe_people(place):
    people = os.listdir(place)
    return people



def main():

    folder = "universe"
    for count, filename in enumerate(os.listdir(folder)):
        new = f"NEW THANOS {str(count)}.jpg"
        old =f"{folder}/{filename}" 
        new =f"{folder}/{new}"
        os.rename(old, new)
 

if __name__ == '__main__':
     
    
    main()
